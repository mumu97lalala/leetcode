var maxSubArray = function (nums) {
    let sum = 0;
    let ans = sum;
    for (let i = 0; i < nums.length; i++) {
        if (sum > 0) {
            sum += nums[i];
        } else {
            sum = nums[i];
        }
        if (ans < sum) {
            ans = sum;
        }
    }
    return ans;
};
